<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Sam</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body style="padding-top:100px" >
    <div class="navbar navbar-fixed-top" style="background-color:black">
        <div class="container-fluid">
            <div class="navbar-header">

                <div class="navbar-left" style="padding-right:5px">
                    <button type="button" id="sidebarCollapse" class="btn btn-info">
                        <i class="fa fa-align-justify"></i> <span>Menu</span>
                    </button>

                    <!--<a class="navbar-brand" href="#">Navbar</a> -->
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>
                <div class="navbar-left">
                    <a href="#" class="navbar-brand" style="color:white">WAVE</a>
                </div>
                <div class="navbar-right">

                    <a href="/" class=" navbar-brand" target="blank" style="color:white">Logout</a>

                </div>


                <div class="navbar-collapse collapse" ">
                    <ul class="nav navbar-nav">
                        @*<li>@Html.ActionLink("Home", "Index", "Home")</li>*@

                    </ul>

                </div>
            </div>
            <div>

            </div>
        </div>
    </div>
    <div class="container-fluid" >

        @RenderBody()
        <hr />

        <footer>
            @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
